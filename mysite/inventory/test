    try {
        var changeLinkColor = function () {
            document.getElementById("ad-feedback-text-ad-endcap-1").style.color = '#111111';
            document.getElementById("ad-feedback-sprite-ad-endcap-1").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/info_icon_1Xsprite.png") no-repeat 0px -12px';
