        <div id="ape_Detail_desktop-detail-ilm_desktop_placement_Feedback" style="position: relative; height: 14px; top: -4px; right: 5px; padding-right: 980px;"><script>(function (window, document, Math) {
        try {
            var changeLinkColor = function () {
                document.getElementById("ad-feedback-text-desktop-detail-ilm").style.color = '#111111';
                document.getElementById("ad-feedback-sprite-desktop-detail-ilm").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/info_icon_1Xsprite.png") no-repeat 0px -12px';
            }
            var onClick = function() {
                 P.when('A').execute( function(A) {
                     A.on( 'a:popover:beforeShow', function() {
                         if(window.uet) {
                             window.uet('bb', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                         }
                     });
                     A.on( 'a:popover:ajaxSuccess', function() {
                         if(window.ue && window.ue.count) {
                             window.ue.count("adFeedback:desktop:feedbackForm:success", 1);
                         }
                         if(window.uet) {
                             window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                         }
                         if(window.uex) {
                             window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                         }
                     });
                     A.on( 'a:popover:ajaxFail', function() {
                         if(window.ue && window.ue.count) {
                             window.ue.count("adFeedback:desktop:feedbackForm:failure", 1);
                         }
                         if(window.uet) {
                             window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                         }
                         if(window.uex) {
                             window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                         }
                     });
                 });
            }
            var defaultLinkBehaviour = function() {
                document.getElementById("ad-feedback-text-desktop-detail-ilm").style.textDecoration = 'none';
                document.getElementById("ad-feedback-text-desktop-detail-ilm").style.color = '#555555';
                document.getElementById("ad-feedback-sprite-desktop-detail-ilm").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/info_icon_1Xsprite.png") no-repeat scroll 0px 0px';
            }
    window.changeLinkColor6c2b1002275747c4b52238b6f837f00f = changeLinkColor;
    window.defaultLinkBehaviour6c2b1002275747c4b52238b6f837f00f = defaultLinkBehaviour;
    window.onClick6c2b1002275747c4b52238b6f837f00f = onClick;
        } catch(ex) {
            if(window.ueLogError) {
                var additionalInfo = {
                    logLevel : 'ERROR',
                    attribution : 'Ad Feedback',
                    message : 'Error in Feedback js '
                };
                window.ueLogError(ex, additionalInfo);
            }
        }
