    <div id="ape_Detail_ams-detail-right-v2_desktop_placement_Feedback" style="position: relative; height: 14px; top: 2px;"><script>(function (window, document, Math) {
    try {
        var changeLinkColor = function () {
            document.getElementById("ad-feedback-text-ams-detail-right-v2").style.color = '#111111';
            document.getElementById("ad-feedback-sprite-ams-detail-right-v2").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/info_icon_1Xsprite.png") no-repeat 0px -12px';
        }
        var onClick = function() {
             P.when('A').execute( function(A) {
                 A.on( 'a:popover:beforeShow', function() {
                     if(window.uet) {
                         window.uet('bb', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
                 A.on( 'a:popover:ajaxSuccess', function() {
                     if(window.ue && window.ue.count) {
                         window.ue.count("adFeedback:desktop:feedbackForm:success", 1);
                     }
                     if(window.uet) {
                         window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                     if(window.uex) {
                         window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
                 A.on( 'a:popover:ajaxFail', function() {
                     if(window.ue && window.ue.count) {
                         window.ue.count("adFeedback:desktop:feedbackForm:failure", 1);
                     }
                     if(window.uet) {
                         window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                     if(window.uex) {
                         window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
             });
        }
        var defaultLinkBehaviour = function() {
            document.getElementById("ad-feedback-text-ams-detail-right-v2").style.textDecoration = 'none';
            document.getElementById("ad-feedback-text-ams-detail-right-v2").style.color = '#555555';
            document.getElementById("ad-feedback-sprite-ams-detail-right-v2").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/info_icon_1Xsprite.png") no-repeat scroll 0px 0px';
        }
window.changeLinkColorc4a28cbb85c04aaf9cba2994c565a058 = changeLinkColor;
window.defaultLinkBehaviourc4a28cbb85c04aaf9cba2994c565a058 = defaultLinkBehaviour;
window.onClickc4a28cbb85c04aaf9cba2994c565a058 = onClick;
    } catch(ex) {
        if(window.ueLogError) {
            var additionalInfo = {
                logLevel : 'ERROR',
                attribution : 'Ad Feedback',
                message : 'Error in Feedback js '
            };
            window.ueLogError(ex, additionalInfo);
        }
    }
})(window, document, Math);</script><a data-a-modal="{&quot;url&quot;:&quot;/af/feedback-form?pl=%7B%22adPlacementMetaData%22%3A%7B%22adElementId%22%3A%22ape_Detail_ams-detail-right-v2_desktop_placement%22%2C%22pageType%22%3A%22Detail%22%2C%22pageUrl%22%3A%22aHR0cHM6Ly93d3cuYW1hem9uLmNhL2RwL0IwN0dGUkNONlIvPw%3D%3D%22%2C%22slotName%22%3A%22ams-detail-right-v2%22%7D%2C%22adCreativeMetaData%22%3A%7B%22adProgramId%22%3A%221027%22%2C%22adCreativeId%22%3A%220%22%2C%22adId%22%3A%22300112083527402%22%2C%22adNetwork%22%3A%22cs%22%7D%7D&amp;daFlg=false&amp;ie=UTF-8&quot;,     &quot;name&quot;:&quot;shared-placement-feedback-modal-ams-detail-right-v2&quot;,&quot;header&quot;:&quot;Leave feedback&quot;,&quot;width&quot;:&quot;460&quot;}" style="position: absolute; top: 2px; right: 0px; display: inline-block; font-size: 11px; color: rgb(85, 85, 85);
